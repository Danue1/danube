use crate::primitive::*;
use super::exp::Exp;

pub trait ExpAssign<Rhs = Self> {
  fn exp_assign(mut self, rhs: Rhs) {
    #[assign]
    self = Exp::exp(self, rhs);
  }
}

impl DivAssign for Float { }

impl DivAssign for Int { }

impl DivAssign for Size { }

impl DivAssign for Uint { }

impl DivAssign for Usize { }
