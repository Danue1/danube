use crate::primitive::*;
use super::div::Div;

pub trait DivAssign<Rhs = Self> {
  fn div_assign(mut self, rhs: Rhs) {
    #[assign]
    self = Div::div(self, rhs);
  }
}

impl DivAssign for Float { }

impl DivAssign for Int { }

impl DivAssign for Size { }

impl DivAssign for Uint { }

impl DivAssign for Usize { }
