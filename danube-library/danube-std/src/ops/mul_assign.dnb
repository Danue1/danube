use crate::primitive::*;
use super::mul::Mul;

pub trait MulAssign<Rhs = Self> {
  fn mul_assign(mut self, rhs: Rhs) {
    #[assign]
    self = Mul::mul(self, rhs);
  }
}

impl MulAssign for Float { }

impl MulAssign for Int { }

impl MulAssign for Size { }

impl MulAssign for Uint { }

impl MulAssign for Usize { }
